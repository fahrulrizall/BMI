@model List<RmDetailModel>
@{
    ViewData["Title"] = "Fair Trade";
}
<div class="content-wrapper">
    <section class="content-header">
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">

                            <form asp-controller="FT" asp-action="Detail" method="get">
                                <div class="input-group input-group-sm">
                                    <input type="date" asp-for="First().start_date" id="start-date" value="@DateTime.Now">
                                    <input type="date" asp-for="First().finish_date" id="finish-date" value="@DateTime.Now">
                                    <span class="input-group-append">
                                        <button type="submit" value="Create" class="btn btn-primary go-repack">Go!</button>
                                    </span>
                                    
                                </div>
                            </form>

                        </div>
                    </div>
                </div>



                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Fair Trade Report</h3>
                        <div class="card-tools ">
                        </div>
                    </div>

                    <div class="card-body">
                        <table id="example1" class="table table-bordered table-striped table-sm">
                            <thead>
                                <tr>
                                    <th style="width: 10%;font-size:small">
                                        Material
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var obj in Model)
                                {
                                <tr>
                                    <td class="text text-sm"><a style="color:black" asp-controller="FT" asp-action="Raw" asp-route-raw="@obj.raw_source">@obj.raw_source</a> </td>
                                </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}